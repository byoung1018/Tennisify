<h1>Login</h1>
<%= render "layouts/errors" %>
<form action="<%= session_url %>" method="post">
<input
  type="hidden"
  name="authenticity_token"
  value="<%= form_authenticity_token %>">

  <label>username or email
    <input type="text"
      name="user[login]"
      id="email"
      value="">
  </label>

  <label>Password
    <input type="text"
      name="user[password]"
      id="password"
      value="">
  </label>


  <input type="submit" value="Login">
</form>
